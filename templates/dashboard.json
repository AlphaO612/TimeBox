{
  "profile": {
    "hidden": false,
    "id": "profile",
    "name": "Профиль",
    "link": "/dashboard",
    "content": {
      "half": true,
      "items": [
        {
          "type": "multiline",
          "arguments": "required",
          "content": {
            "name": "Логин",
            "input": {
              "name": "login",
              "type": "text",
              "placeholder": "Логин",
              "min": 6,
              "max": 12,
              "value": "-@userData|login<#>"
            },
            "button": {
              "value": "Изменить"
            }
          }
        },
        {
          "type": "multiline",
          "content": {
            "name": "Пароль",
            "text": {
              "value": "********",
              "placeholder": "Пароль"
            },
            "a": {
              "value": "Изменить",
              "link": "/dashboard?page=changePassword"
            }
          }
        }
      ]
    }
  },
  "setting": {
    "id": "setting",
    "hidden": false,
    "name": "Настройки сайта",
    "link": "/dashboard?page=setting",
    "content": {
      "half": false,
      "items": [
        {
          "type": "multiline",
          "content": {
            "name": "API-Система",
            "text": {
              "value": "-@numberApi<#> подключенных устройств",
              "placeholder": ""
            },
            "a": {
              "value": "Изменить",
              "link": "/dashboard/api"
            }
          }
        },
        {
          "type": "multiline",
          "content": {
            "name": "Контролирование расписания",
            "text": {
              "value": "-@userData|statusTimeBox->Доступно/Не доступно<#>",
              "placeholder": ""
            },
            "a": {
              "value": "-@userData|statusTimeBox->Открыть панель/Подать<#>",
              "link": "/dashboard/timebox"
            }
          }
        },
        {
          "type": "title",
          "content": {
            "value": "Сбор информации"
          }
        },
        {
          "type": "multiline",
          "content": {
            "name": "Показать все хранимые о вас данные?",
