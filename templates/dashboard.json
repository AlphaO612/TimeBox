      "half": true,
      "items": [
        {
          "type": "multiline",
          "content": {
            "name": "Логин",
            "input": {
              "name": "login",
              "type": "text",
              "placeholder": "Логин",
              "min": 6,
              "max": 12,
              "value": "-@userData|login<#>"
            },
            "button": {
              "value": "Изменить"
            }
          }
        },
        {
          "type": "multiline",
          "content": {
            "name": "Пароль",
            "text": {
              "value": "********",
              "placeholder": "Пароль"
            },
            "a": {
              "value": "Изменить",
              "link": "/dashboard?page=changePassword"
            }
          }
        }
      ]
    }
  },
  "setting": {
    "id": "setting",
    "hidden": false,
    "name": "Настройки сайта",
    "link": "/dashboard?page=setting",
    "content": {
      "half": false,
      "items": [
        {
          "type": "multiline",
          "content": {
            "name": "API-Система",
            "text": {
              "value": "-@userData|api|number<#> подключенных устройств",
