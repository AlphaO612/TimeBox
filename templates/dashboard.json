            "a": {
              "value": "Показать",
              "link": "/dashboard?page=showInfo"
            }
          }
        },
        {
          "type": "button",
          "content": {
            "a": {
              "value": "Удалить аккаунт",
              "link": "/dashboard?page=deleteAll",
              "style": "border-color:#ff2020; color:#f01010;"
            }
          }
        }
      ]
    }
  },
  "logOut": {
    "id": "logOut",
    "hidden": false,
    "name": "Выйти",
    "link": "/check?loseLog=1",
    "style": "color:#ff0000;"
  },
